NCA <- function(data,ID,TIME,DV,DOSE,adminType="extravascular",Infusion_duration){
data1 <- data %>%
dplyr::rename(
ID = !!sym(ID),
TIME = !!sym(TIME),
DV = !!sym(DV),
)
if(adminType=="extravascular"){
out <- ncappc(obsFile=data1,
onlyNCA = T,
extrapolate = T,
printOut = F,
evid = FALSE,
noPlot = T,
idNmObs = "ID",
timeNmObs = "TIME",
adminType ="extravascular",
method = "linearup-logdown",
concNmObs ="DV",
doseAmtNm = "DOSE")}
if(adminType=="iv-infusion"){
out <- ncappc(obsFile=data1,
onlyNCA = T,
extrapolate = T,
printOut = F,
evid = FALSE,
noPlot = T,
idNmObs = "ID",
timeNmObs = "TIME",
adminType ="iv-infusion",
method = "linearup-logdown",
concNmObs ="DV",
doseAmtNm = "DOSE",
TI=Infusion_duration)}
if(adminType=="iv-bolus"){
out <- ncappc(obsFile=data1,
onlyNCA = T,
extrapolate = T,
printOut = F,
evid = FALSE,
noPlot = T,
idNmObs = "ID",
timeNmObs = "TIME",
adminType ="iv-bolus",
method = "linearup-logdown",
concNmObs ="DV",
doseAmtNm = "DOSE")}
out <- data.frame(out$ncaOutput)
return(NCA=out)
}
NAC <- NCA(data=data,TIME = "TIME",ID = "ID",DV="DV")
